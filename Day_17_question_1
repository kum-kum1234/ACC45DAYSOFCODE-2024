#include <iostream>
using namespace std;

int main() {
    int T; // number of test cases
    cin >> T;

    while (T--) {
        int X, Y; // travel times with bike and car respectively
        cin >> X >> Y;

        if (X < Y) {
            cout << "BIKE" << endl;
        } else if (X > Y) {
            cout << "CAR" << endl;
        } else {
            cout << "SAME" << endl;
        }
    }

    return 0;
}
